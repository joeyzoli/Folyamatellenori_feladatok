<?xml version="1.0" encoding="utf-8"?>

<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".MainActivity"
    tools:ignore="Deprecated">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="224dp"
        android:text="@string/folyamatellen_r" />

    <Spinner
        android:id="@+id/nev_box"
        android:layout_width="211dp"
        android:layout_height="48dp"
        android:layout_x="162dp"
        android:layout_y="208dp"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="67dp"
        android:layout_y="294dp"
        android:text="@string/instruktor" />

    <EditText
        android:id="@+id/instruktor_mezo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="163dp"
        android:layout_y="271dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        tools:ignore="LabelFor,SpeakableTextPresentCheck"
        android:autofillHints="" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="39dp"
        android:layout_y="366dp"
        android:text="@string/m_szakvezet" />

    <EditText
        android:id="@+id/muvez_mezo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="163dp"
        android:layout_y="354dp"
        android:autofillHints=""
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        tools:ignore="LabelFor,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/ellenorzes_gomb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="140dp"
        android:layout_y="533dp"
        android:onClick="ujoldal"
        android:text="@string/start" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="90dp"
        android:layout_y="161dp"
        android:text="@string/d_tum" />

    <EditText
        android:id="@+id/datum_mezo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="163dp"
        android:layout_y="148dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="449dp"
        android:text="@string/m_szak" />

    <RadioGroup
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_x="158dp"
        android:layout_y="434dp"
        android:orientation="horizontal">

        <RadioButton
            android:id="@+id/csekk_de"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/de" />

        <RadioButton
            android:id="@+id/csekk_du"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/du" />

        <RadioButton
            android:id="@+id/csekk_ej"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/j" />
    </RadioGroup>

</AbsoluteLayout>
